resource "tailor_secretmanager_vault" "default" {
  workspace_id = tailor_workspace.ims.id
  name         = "default"
}

resource "tailor_secretmanager_secret" "saml-cert" {
  workspace_id = tailor_workspace.ims.id
  vault_name   = tailor_secretmanager_vault.default.name

  name             = "saml-cert"
  value_wo         = ""
  value_wo_version = 1
}

resource "tailor_secretmanager_secret" "saml-key" {
  workspace_id = tailor_workspace.ims.id
  vault_name   = tailor_secretmanager_vault.default.name

  name             = "saml-key"
  value_wo         = ""
  value_wo_version = 1
}